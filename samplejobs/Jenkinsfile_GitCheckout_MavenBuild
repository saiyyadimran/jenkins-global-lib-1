@Library('global-shared-pipeline-lib-new') _
pipeline {
podTemplate(
containers: [
  containerTemplate(name: 'maven', image: 'maven:3.6.1-jdk-8', ttyEnabled: true, command: 'cat')
]) 

		agent any
		stages {
			stage('Git Checkout') {
				steps {
					gitCheckout(
						branch: "master",
						url: "https://github.com/saiyyadimran/jenkins-global-lib",
						credentialsId: "GIT1"
					)
				}
			}
			stage('Maven Clean Build') {
				steps {
					container('maven') {
						sh "pwd"
						sh "ls -ltr"
						sh "cd samplemavenproject"
						sh "ls -ltr"
						sh "pwd"
						mvnCleanInstallWithoutProfile(
						  skipTest: true
						)
						sh "ls -ltr"
					}
				}
			}
		}

}
