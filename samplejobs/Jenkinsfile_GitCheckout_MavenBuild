@Library('global-shared-pipeline-lib-new') _
 
pipeline {
    agent any
    stages {
        stage('Git Checkout') {
            steps {
                gitCheckout(
                    branch: "master",
                    url: "https://github.com/saiyyadimran/jenkins-global-lib",
                    credentialsId: "GIT1"
                )
            }
        }
        stage('Maven Clean Build') {
            steps {
                sh "cd ${WORKSPACE}/samplemavenproject"
                sh "ls -ltr"
                sh "pwd"
                mvnCleanInstallWithoutProfile(
                  skipTest: true
                )
                sh "ls -ltr"
            }   
        }
    }
}
